{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Discord Onyx",
  "author": "zangetsu02",
  "themes": [
    {
      "name": "Discord Onyx",
      "appearance": "dark",
      "style": {
        "border": "#222225",
        "border.variant": "#2f363d",
        "border.focused": "#5197ed",
        "border.selected": "#5197ed",
        "border.transparent": "#00000000",
        "border.disabled": "#242428",

        "elevated_surface.background": "#070709",
        "surface.background": "#070709",
        "background": "#000000",

        "element.background": "#070709",
