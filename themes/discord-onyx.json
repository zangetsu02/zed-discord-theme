        "text.accent": "#5197ed",

        "icon": "#b5b4b4",
        "icon.muted": "#959da5",
        "icon.disabled": "#6d6d73",
        "icon.placeholder": "#959da5",
        "icon.accent": "#5197ed",

        "status_bar.background": "#000000",
        "title_bar.background": "#000000",
        "title_bar.inactive_background": "#070709",
        "toolbar.background": "#070709",

        "tab_bar.background": "#070709",
