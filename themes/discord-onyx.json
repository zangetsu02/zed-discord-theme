        "modified": "#6394bf",

        "players": [
          {
            "cursor": "#5197ed",
            "background": "#5197ed",
            "selection": "#5197ed3d"
          },
          {
            "cursor": "#4d9375",
            "background": "#4d9375",
            "selection": "#4d93753d"
          },
          {
            "cursor": "#d9739f",
            "background": "#d9739f",
            "selection": "#d9739f3d"
          },
          {
            "cursor": "#e6cc77",
            "background": "#e6cc77",
            "selection": "#e6cc773d"
          }
        ],

        "syntax": {
          "attribute": { "color": "#b8a965" },
          "boolean": { "color": "#4d9375" },
          "comment": { "color": "#758575dd" },
          "constant": { "color": "#c99076" },
          "constructor": { "color": "#4d9375" },
          "embedded": { "color": "#b5b4b4" },
          "enum": { "color": "#e6cc77" },
          "function": { "color": "#80a665" },
          "keyword": { "color": "#4d9375" },
          "link_text": { "color": "#5197ed", "font_style": "italic" },
          "link_uri": { "color": "#5eaab5" },
          "number": { "color": "#4c9a91" },
          "operator": { "color": "#cb7676" },
          "property": { "color": "#b8a965" },
          "punctuation": { "color": "#666666" },
          "string": { "color": "#c98a7d" },
          "string.regex": { "color": "#c4704f" },
          "string.escape": { "color": "#e6cc77" },
          "tag": { "color": "#4d9375" },
          "type": { "color": "#5d99a9" },
          "variable": { "color": "#bd976a" },
          "title": { "color": "#b5b4b4", "font_weight": 700 },
          "emphasis": { "color": "#b5b4b4", "font_style": "italic" },
          "emphasis.strong": { "color": "#b5b4b4", "font_weight": 700 }
        }
